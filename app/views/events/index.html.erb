<h1>All Events</h1>

<% if @events.any? %>
    <ul>
        <% @events.each do |event| %>
            <li>Event <%= "#{event.id} - #{event.date}" %></li>
        <% end %>
    </ul>
<% else %>
    <p> No events found.</p>
<% end %>

<% if @user %>
    <h3><%= link_to 'Your Events', user_path(@user) %></h3 >
<% end %>